   "outputs": [],
   "source": [
    "def timeit(fn):\n",
    "    def wrapper(*args, **kwargs):\n",
    "        start = time()\n",
    "        res = fn(*args, **kwargs)\n",
    "        print(fn.__name__, \"took\", time() - start, \"seconds.\")\n",
    "        return res\n",
    "    return wrapper"
   ]
  },
